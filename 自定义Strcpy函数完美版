#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<string.h>
char* my_strcpy(char* dest,const char* src)
{
	char* ret = dest;
	assert(dest != NULL);//断言
	assert(src != NULL);//断言
	//把src指向的字符拷贝到dest指向的空间，包含'\0'字符
		while (*dest++ = *src++)
		{
			;
		}
		return ret;
}
int main()
{
	char arr1[] = "############";
	char arr2[] = "bit";
	printf("%s", my_strcpy(arr1, arr2));
	return 0;
}
