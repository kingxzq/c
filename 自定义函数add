#include<stdio.h>
int Add(int x, int y)
{
    int z =x+y;
    return z;
}
int main()
{
    int num1=10;
    int num2=20;
    int sum=0;
    int sum1=0;
    int sum2=0;
    int a=100;
    int b=200;
    sum=Add(a,b);
    sum1=Add(num1,num2);
    sum2=Add(2,3);
    printf("sum=%d\n",sum);
    printf("sum1=%d\n",sum1);
    printf("sum2=%d\n",sum2);
    return 0;
}
