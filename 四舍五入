#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>


int main() {
    float i = 0;
    scanf("%f", &i);
    float j = i-(int)i;
    if (i > 0) {
        if (j >= 0.5)
        {
            int k = (int)i + 1;
            printf("%d", k);
        }
        else
            printf("%d", (int)i);
    }
    else {
        if (j <=-0.5)
        {
            printf("%d", (int)i-1);
        }
        else
        {
            printf("%d", (int)i);
        }
    }
    return 0;
}
